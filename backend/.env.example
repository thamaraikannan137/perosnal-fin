# Node Environment
NODE_ENV=development
PORT=3000

# MongoDB Connection (Update with your MongoDB URI)
MONGODB_URI=mongodb://localhost:27017/personal_finance

# JWT Secrets (32+ characters required - GENERATE YOUR OWN!)
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters-long-here
JWT_REFRESH_SECRET=your-super-secret-refresh-jwt-key-minimum-32-characters-long
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
